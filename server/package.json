{
  "name": "nubes_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "start:dev": "nodemon ./src/server.ts",
    "build": "tsc",
    "docker:image": "docker build -f Dockerfile -t jdcamacho/nube-server .",
    "docker:cnt:unix": "docker run -t -p 3000:3000 -d -v $(pwd):/usr/app/server --name nube-server jdcamacho/nube-server",
    "docker:cnt:pshell": "docker run -t -p 3000:3000 -d -v ${PWD}:/usr/app/server --name nube-server jdcamacho/nube-server",
    "docker:cnt:rm": "docker image rm -f jdcamacho/nube-server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ws": "^8.5.5",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
